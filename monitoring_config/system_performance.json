{
  "id": null,
  "uid": "system-performance",
  "title": "System Performance",
  "tags": [
    "trading",
    "autonomous"
  ],
  "timezone": "UTC",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
