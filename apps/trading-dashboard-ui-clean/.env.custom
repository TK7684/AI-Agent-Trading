# Custom API Configuration
# Replace these with your actual API endpoints

# Your Trading API Base URL
VITE_API_BASE_URL=https://your-trading-api.com/api
VITE_WS_BASE_URL=wss://your-trading-api.com/ws

# Alternative API endpoints
VITE_BACKEND_API_URL=https://your-trading-api.com/api
VITE_BACKEND_WS_URL=wss://your-trading-api.com/ws

# Authentication settings
VITE_AUTH_ENABLED=true
VITE_DEBUG_MODE=false

# Environment
VITE_ENVIRONMENT=production
VITE_APP_NAME=Trading Dashboard
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_CONNECTION_TEST=true
VITE_ENABLE_MOCK_DATA=false